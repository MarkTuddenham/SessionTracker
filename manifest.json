  {
    "name": "Tab Sessions",
    "version": "0.1",
    "description": "Keeps track of your tabs for you!",
    "permissions": [
      "tabs",
      "activeTab",
      "declarativeContent",
      "storage"
    ],
    "background": {
      "persistent": false,
      "scripts": [
        "log.js",
        "background.js"
      ]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/book-16.png",
        "32": "images/book-32.png",
        "48": "images/book-48.png",
        "128": "images/book-128.png"
      }
    },
    "icons": {
      "16": "images/book-16.png",
      "32": "images/book-32.png",
      "48": "images/book-48.png",
      "128": "images/book-128.png"
    },
    "manifest_version": 2
  }